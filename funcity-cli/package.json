{
  "name": "funcity-cli",
  "version": "0.0.1",
  "description": "A functional language interpreter with text processing",
  "keywords": [
    "functional",
    "language",
    "interpreter",
    "text-processing",
    "cli",
    "repl"
  ],
  "author": "Kouji Matsui (@kekyo@mi.kekyo.net)",
  "license": "MIT",
  "type": "module",
  "bin": {
    "funcity": "./dist/index.cjs"
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "vite build",
    "dev": "echo \"funcity-cli does not have dev rule.\"",
    "test": "npm run build && vitest run",
    "pack": "npm run build && screw-up pack --pack-destination ../artifacts/"
  },
  "dependencies": {
    "commander": ">=11.0.0",
    "funcity": "*"
  },
  "devDependencies": {
    "@types/node": ">=20.0.0",
    "prettier-max": ">=1.13.0",
    "screw-up": ">=1.26.0",
    "typescript": ">=5.0.0",
    "vite": ">=5.0.0",
    "vite-plugin-dts": ">=3.0.0",
    "vitest": ">=1.0.0"
  }
}
